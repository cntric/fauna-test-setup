{
  "name": "fauna-dev-tests",
  "version": "0.1.0",
  "description": "A package for running tests against the Fauna development server.",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "jest --verbose --coverage --maxWorkers=50% ./src",
    "perf": "jest --verbose --coverage --maxWorkers=50% --testRegex 'src/(?:.*)\\.perf\\.(tsx|ts)'",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./tsconfig-fixup",
    "docs": "rm -fr docs/* && npx typedoc src --categorizeByGroup false && http-server ./docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "babel-jest": "^27.1.1",
    "determinismus": "https://github.com/concentricio/determinismus",
    "jest": "^27.1.1",
    "nodemon": "^2.0.12",
    "tulleries": "https://github.com/concentricio/tulleries",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@types/dockerode": "^3.3.0",
    "@types/node": "^16.9.1",
    "@types/shortid": "^0.0.29",
    "child_process": "^1.0.2",
    "dockerode": "^3.3.1",
    "fauna-shell": "^0.12.6",
    "faunadb": "^4.4.1",
    "http-server": "^13.0.2",
    "shortid": "^2.2.16",
    "typedoc": "^0.22.4"
  }
}
